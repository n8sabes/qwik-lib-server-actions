{
  "routes": [
    {
      "route": "/build/*"
    },
    {
      "route": "/favicon.svg"
    },
    {
      "route": "/manifest.json"
    },
    {
      "route": "/service-worker.js"
    },
    {
      "route": "/.auth/*"
    },
    {
      "route": "/*",
      "rewrite": "/api/render"
    }
  ],
  "navigationFallback": {
    "rewrite": "/api/render"
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
